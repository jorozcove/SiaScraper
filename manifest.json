{
    "name": "Sia Scraper",
    "description": "Una herramienta que le permitirá obtener la información de los grupos ofertados en las asignaturas publicadas en el SIA de la Universidad Nacional de Colombia",
    "version": "1.0.1",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Sia Scraper",
      "default_icon": "/Assets/Logo-SiaScraper.png"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "icons": { "800": "/Assets/Logo-SiaScraper.png"}
  }